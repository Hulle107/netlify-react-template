{
  "name": "netlify-react-template",
  "version": "1.0.1",
  "private": true,
  "description": "A Netlify & React template",
  "keywords": [
    "netlify",
    "react",
    "lambda-api"
  ],
  "license": "MIT",
  "scripts": {
    "start": "run-p start:**",
    "start:api": "npm run start --workspace=api --if-present",
    "start:client": "npm run start --workspace=client --if-present",
    "build": "run-p build:**",
    "build:api": "npm run build --workspace=api --if-present",
    "build:client": "npm run build --workspace=client --if-present"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5"
  },
  "workspaces": [
    "api",
    "client"
  ]
}
